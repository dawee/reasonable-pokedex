{
  "version": 2,
  "builds": [
    {
      "src": "src/micro.js",
      "use": "@now/node"
    },
    {
      "src": "index.html",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "index.html"
    },
    {
      "src": "/graphql",
      "dest": "src/micro.js"
    }
  ]
}